{
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "vueIndentScriptAndStyle": false,
    "endOfLine": "lf",
    "parser": "vue",
    "overrides": [
        {
            "files":["src/**/*.js", "*.js"],
            "options":{
                "parser":"babel",
                "semi": false,
                "tabWidth": 2,
                "singleQuote": false,
                "printWidth": 80
            }
        },
        {
            "files":["src/**/*.ts"],
            "options":{
                "parser":"babel-ts",
                "semi": false,
                "tabWidth": 2,
                "singleQuote": false,
                "printWidth": 80
            }
        },
        {
            "files":[".prettierrc", "*.json"],
            "options": {
                "parser":"json",
                "tabWidth": 2,
                "singleQuote": false,
                "printWidth": 80
            }
        }
    ]
}